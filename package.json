{
  "name": "mcp-dev-blueprints",
  "version": "1.0.1",
  "description": "Enterprise application patterns MCP server with HTTP transport",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mcp-dev-blueprints": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "build:with-dev": "tsc -p tsconfig.dev.json",
    "watch": "tsc --watch",
    "watch:dev": "tsc -p tsconfig.dev.json --watch",
    "start": "node dist/index.js",
    "dev": "npm run build:with-dev && node dist/dev/index.js --mode http --kb-path dev/knowledge_base",
    "dev:http": "npm run build:with-dev && node dist/dev/index.js --mode http --kb-path dev/knowledge_base",
    "dev:stdio": "npm run build:with-dev && node dist/dev/index.js --mode stdio --kb-path dev/knowledge_base",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "semantic-release": "semantic-release",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.2",
    "express": "^4.18.2",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.24",
    "@types/supertest": "^6.0.3",
    "@types/yargs": "^17.0.33",
    "copyfiles": "^2.4.1",
    "jest": "^30.2.0",
    "rimraf": "^5.0.5",
    "semantic-release": "^22.0.12",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.19.0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "server",
    "express",
    "enterprise-patterns"
  ],
  "author": "Paulo Baima <psbds_filho@hotmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/psbds/mcp-dev-blueprints.git"
  },
  "homepage": "https://github.com/psbds/mcp-dev-blueprints#readme",
  "bugs": {
    "url": "https://github.com/psbds/mcp-dev-blueprints/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
